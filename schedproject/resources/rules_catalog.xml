    <!--===-->
    <latex>
        <entry>requiredRooms(\SESSION',\ROOM')</entry>
        <setEntry>\ROOM' \subseteq \ROOM</setEntry>
        <equation
            >\forall s \in \SESSION', \ROOM' \subseteq \var{\SESSION}{\ROOM}{s}</equation>
    </latex>
    <!--===-->
