<constraint>
    <id>SESSION_WORLOAD</id>
    <name>session_workload</name>
    <arity>1</arity>
    <parameters single="true">
         <parameter
                name="scope"
                type="timeScope"
            > The workload should be computed for every week or every day</parameter>
         <parameter name="value" type="slotRange"> min-max</parameter>
         <parameter
                name="unit"
                type="time"
            > Should the value be a number of sessions or slots</parameter>
    </parameters>
    <keywords>time, slot, count, workload</keywords>
    <!--===-->
    <latex>
        <entry>session_workload(\SESSION', w_1, w_2)</entry>
        <setEntry>w_1, w_2 \in \SLOT \cup \{0\}</setEntry>
        <equation
            >\forall d \in \WEEKDAY, w_1 \leq |\{s \in \SESSION' : \var{\SESSION}{\WEEKDAY}{s} = d\}| \leq w_2</equation>
    </latex>
    <!--===-->
    <description lang="en">
        <short>The number of sessions per time frame is bounded</short>
        <long />
    </description>
    <description lang="fr">
        <short>Le nombre de séances par cadre temporel est limité</short>
        <long />
    </description>
    <!--===-->
    <miscellaneous
        >Essentiel pour maintenir une charge de travail équilibrée et gérable</miscellaneous>
    <versions status="valable">
        <version num="0.1">init</version>
    </versions>
</constraint>


<constraint>
