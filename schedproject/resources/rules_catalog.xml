<constraint>
    <id>COMPACTNESS</id>
    <name>compactness</name>
    <arity>1</arity>
    <parameters single="true">
        <parameter
                name="compactnessRange"
                type="compactnessRange"
            >sigma value representing the compactness range allowed for sessions.</parameter>
    </parameters>
    <keywords>ressource, repartition, teacher, assign</keywords>
    <!--===-->
    <latex>
        <entry>compactness(\SESSION',\sigma)</entry>
        <setEntry>\sigma \in 0..|\SLOT'|</setEntry>
        <equation
            >\forall d \in \WEEKDAY : \exists \SESSION'' = \{s \in \SESSION' : \var{\SESSION}{\WEEKDAY}{s}\} \land ((\max_{s \in \SESSION''}(\var{\SESSION}{\SLOT}{s}+\sessionduration{s})-\min_{s \in \SESSION'}(\var{\SESSION}{\SLOT}{s}))- \sum_{s \in \SESSION''}\sessionduration{s})) / (|S''|-1) \leq \sigma</equation>
    </latex>
    <!--===-->
    <description lang="en">
        <short>The sessions makespan is bounded</short>
        <long />
    </description>
    <description lang="fr">
        <short>La durée totale des séances est limitée</short>
        <long />
    </description>
    <!--===-->
    <miscellaneous
        >Optimiser l'emploi du temps pour réduire les temps morts</miscellaneous>
    <versions status="valable">
        <version num="0.1">init</version>
    </versions>
</constraint>



