    <constraint>
        <id>ADJACENT_ROOMS</id>
        <name>adjacent_rooms</name>
        <arity>1</arity><!-- fix : chiffre >= 1 : +  -->
        <parameters single="true">
            <parameter
                name="roomChain"
                type="roomlist"
                mandatory="1"
            >comma separated list of room identifiers. One roomChain defines adjacent rooms in order.</parameter>
        </parameters>
        <metrics>
            <metric
                name="distance"
            >Elle calcule la distance entre la valeur réel et celle souhaité</metric>
        </metrics>
        <keywords>ressource, repartition, rooms, tuple</keywords>
        <!--===-->
        <latex>
        <entry>adjacentRooms(\SESSION',K\_1,\dots,K\_n)</entry>
        <setEntry app="latex">K\_i \subseteq \ROOM, i : 1..n</setEntry>
        <equation
                app="latex"
            >\forall s\_1, s\_2 \in \SESSION', \var{\SESSION}{\SLOT}{s\_1} = \var{\SESSION}{\SLOT}{s\_2} \forall i \in {1..n} : C\_i ={ \var{\SESSION}{\ROOM}{s} \mid s \in \SESSION' , \var{\SESSION}{\ROOM}{s} \in K\_i } \land \forall r,r' \in C\_i : \exists path(r,r') \land |{C\_i \mid C\_i \ne \emptyset , i = 1..n }| \leq \sigma</equation>
        </latex>
        <!--===-->
        <description lang="en">
            <short>Sessions must be adjacent in the given rooms</short>
            <long />
        </description>

        <description lang="fr">
            <short
            >Les séances doivent être adjacentes dans les salles données</short>
            <long />
        </description>

        <miscellaneous
        >Attention elle induit des surcouts si trop d'élements</miscellaneous>
        <versions status="valable">
            <version num="0.1">init</version>
        </versions>
    </constraint>

    <constraint>
        <id>ALLOWED_GRIDS</id>
        <name>allowed_grids</name>
        <arity>1</arity>
        <parameters single="false">
            <parameter
                name="grids"
                type="unionGrid"
            >list of starting slots</parameter>
            <!-- <parameter name="dailySlots" type="dailySlot" repetition="false" mandatory="1" >list of starting slots</parameter>
            <parameter name="days" type="day" repetition="false" mandatory="1" >list of allowed days</parameter>
            <parameter name="weeks" type="week" repetition="false" mandatory="1" >list of allowed weeks</parameter> -->
        </parameters>
        <keywords
            lang="en"
        >ressource, repartition, grid, time, set,same</keywords>
        <!--===-->
        <entry>allowedGrids(\SESSION',G)</entry>
        <setEntry
        >G \in (W'\times D' \times M')^n, n \in \mathbb{N}^*, \SESSION'\subseteq\SESSION, W'\subseteq \WEEK, D'\subseteq\WEEKDAY, M'\subseteq\DAILYSLOT</setEntry>
        <equation
        >\forall s\in S', \exists k \in \{1..n\}: \var{\SESSION}{\WEEK}{s}\in W'_k \wedge \var{\SESSION}{\WEEKDAY}{s}\in D'_k \wedge \var{\SESSION}{\DAILYSLOT}{s}\in M'_k</equation>
        <!--===-->
        <description lang="en">
            <short>Sessions may only start in the given time grids</short>
            <long />
        </description>
        <description lang="fr">
            <short
            >Les séances peuvent uniquement débuter dans les grilles horaires spécifiées</short>
            <long />
        </description>
        <!--===-->
        <miscellaneous
        >Attention elle induit des surcouts si trop d'élements</miscellaneous>
        <versions status="valable">
            <version num="0.1">init</version>
        </versions>
    </constraint>

    <constraint>
        <id>ALLOWED_ROOMS</id>
        <name>allowed_rooms</name>
        <arity>1</arity>
        <parameters single="true">
            <parameter
                name="rooms"
                type="roomIds | roomLabels"
            >comma separated list of room identifiers or labels</parameter>
            <parameter
                name="rooms2"
                type="roomIds | roomLabels"
                optionnel="1"
            >comma separated list of room identifiers or labels</parameter>
        </parameters>
        <keywords lang="fr">ressource, repartition, room,set,allowed</keywords>
        <!--===-->
        <entry>allowedRooms(\SESSION',\ROOM')</entry>
        <setEntry
        >\SESSION' \subseteq \SESSION, \ROOM' \subseteq \ROOM</setEntry>
        <equation
        >\forall s \in \SESSION', \var{\SESSION}{\ROOM}{s} \subseteq \ROOM'</equation>
        <!--===-->
        <description lang="en">
            <short>Sessions may only be hosted in the given rooms</short>
            <long />
        </description>
        <description lang="fr">
            <short
            >Les séances peuvent uniquement se tenir dans les salles spécifiées</short>
            <long />
        </description>
        <!--===-->
        <miscellaneous
        >Attention elle induit des surcouts si trop d'élements</miscellaneous>
        <versions status="valable">
            <version num="0.1">init</version>
        </versions>
    </constraint>

<constraint>
    <id>ALLOWED_SLOTS</id>
    <name>allowed_slots</name>
    <arity>1</arity>
    <parameters single="true">
        <parameter
                name="slotlist"
                type="timeSlotlist"
            >comma separated list of time slot identifiers. Each slot represents an allowed time period for sessions.</parameter>
    </parameters>
    <keywords>ressource, repartition, slot, time, set, allowed</keywords>
    <!--===-->
    <latex>
        <entry>allowedSlots(\SESSION',\SLOT')</entry>
        <setEntry>K_i \subseteq \ROOM, i : 1..n</setEntry>
        <equation
            >\forall s \in S' [\var{\SESSION}{\SLOT}{s},\var{\SESSION}{\SLOT}{s}+\sessionduration{s}] \subseteq \SLOT'</equation>
    </latex>
    <!--===-->
    <description lang="en">
        <short>Sessions may only run in the given time slots</short>
        <long />
    </description>
    <description lang="fr">
        <short
            >Les séances peuvent uniquement avoir lieu dans les créneaux horaires spécifiés</short>
        <long />
    </description>
    <!--===-->
    <miscellaneous
        >Attention à la planification des créneaux pour éviter les conflits horaires</miscellaneous>
    <versions status="valable">
        <version num="0.1">init</version>
    </versions>
</constraint>


