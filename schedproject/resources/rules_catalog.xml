    <name>sequenced</name>
    <arity>1</arity>
    <parameters single="true">
    </parameters>
    <keywords>time, slot, sequenced</keywords>
    <!--===-->
    <latex>
        <entry>sequenced(\SESSION'_1, \dots, \SESSION'_n)</entry>
        <setEntry />
        <equation
            >\forall s_i \in \SESSION'_i, \forall s_{i+1} \in \SESSION'_{i+1}, \var{\SESSION}{\SLOT}{s_{i+1}} \geq \var{\SESSION}{\SLOT}{s_i} + \sessionduration{s_i}</equation>
    </latex>
    <!--===-->
    <description lang="en">
        <short>Sessions run sequentially</short>
        <long />
    </description>
    <description lang="fr">
        <short>Les séances se déroulent de manière séquentielle</short>
