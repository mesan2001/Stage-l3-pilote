<constraint>
    <id>REQUIRED_TEACHERS</id>
    <name>required_teachers</name>
    <arity>1</arity>
    <parameters single="true">
        <parameter
                name="requiredTeacherlist"
                type="teacherlist"
            >list of teacher identifiers who must teach the sessions, with optional numeric constraints on their assignment frequencies (Δ).</parameter>
    </parameters>
    <keywords>ressource, teacher, required</keywords>
    <!--===-->
    <latex>
        <entry>requiredTeachers(\SESSION',\TEACHER',\Delta*)</entry>
        <setEntry
            >\TEACHER' \subseteq \TEACHER, \Delta = \{\forall t \in \TEACHER'\mid\delta_{1,t},\delta_{2,t}\},\forall t \in \TEACHER',\forall i \in \{1,2\},\;\delta_{i,t}\in\mathbb{N}</setEntry>
        <equation
            >\forall s \in \SESSION',\; (\var{\SESSION}{\TEACHER}{s} \subseteq \TEACHER'  \land \forall t \in\TEACHER',\; \delta_{1,t} \leq\sum_{s \in \SESSION'} (t \in \var{\SESSION}{\TEACHER}{s})\leq \delta_{2,t} )</equation>
    </latex>
    <!--===-->
    <description lang="en">
        <short>Sessions must be taught by the given teacher(s)</short>
        <long />
    </description>
    <description lang="fr">
        <short
            >Les séances doivent être enseignées par les enseignants indiqués</short>
        <long />
    </description>
    <!--===-->
